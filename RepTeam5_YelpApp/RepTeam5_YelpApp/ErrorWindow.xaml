<Window x:Class="RepTeam5_YelpApp.ErrorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RepTeam5_YelpApp"
        mc:Ignorable="d"
        Title="Error" Height="175" Width="250">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="25*"/>
            <ColumnDefinition Width="24*"/>
        </Grid.ColumnDefinitions>
        <TextBlock x:Name="TextBlock" Margin="10,32,10.2,9.6" TextWrapping="Wrap" Text="TextBlock" TextAlignment="Center" Grid.ColumnSpan="2"/>
    </Grid>
</Window>
